{
  "name": "aicouncil",
  "version": "0.1.0",
  "description": "Multi-model AI collaborative discussion plugin for OpenCode",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --cwd packages/aicouncil-plugin build",
    "dev": "bun run --cwd packages/aicouncil-plugin dev",
    "typecheck": "bun run --cwd packages/aicouncil-plugin typecheck",
    "test": "bun run --cwd packages/aicouncil-plugin test"
  },
  "keywords": [
    "opencode",
    "plugin",
    "ai",
    "multi-model",
    "discussion"
  ],
  "author": "AICouncil Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elfgzp/aicouncil"
  }
}
