{
  "name": "aicouncil",
  "version": "0.1.0",
  "description": "Multi-model AI collaborative discussion plugin for OpenCode",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run --workspace=@aicouncil/opencode-plugin build",
    "dev": "npm run --workspace=@aicouncil/opencode-plugin dev",
    "typecheck": "npm run --workspace=@aicouncil/opencode-plugin typecheck",
    "test": "npm run --workspace=@aicouncil/opencode-plugin test",
    "test:coverage": "npm run --workspace=@aicouncil/opencode-plugin test:coverage",
    "test:smoke": "cd packages/aicouncil-plugin && npm run test:smoke",
    "validate": "./scripts/validate.sh --quick",
    "validate:full": "./scripts/validate.sh --full",
    "validate:ci": "./scripts/validate.sh --ci",
    "verify-build": "./scripts/verify-build.sh"
  },
  "keywords": [
    "opencode",
    "plugin",
    "ai",
    "multi-model",
    "discussion"
  ],
  "author": "AICouncil Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elfgzp/aicouncil"
  }
}
