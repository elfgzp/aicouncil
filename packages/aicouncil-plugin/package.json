{
  "name": "@aicouncil/opencode-plugin",
  "version": "0.1.0",
  "description": "Multi-model AI collaborative discussion plugin for OpenCode",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:bun": "bun build src/index.ts --outdir dist --target bun && tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:real-api": "ENABLE_REAL_API_TESTS=true vitest run src/__tests__/integration/real-api.test.ts",
    "test:integration": "vitest run src/__tests__/integration"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": ">=0.1.0"
  },
  "keywords": [
    "opencode",
    "plugin",
    "ai",
    "multi-model",
    "discussion",
    "council"
  ],
  "author": "AICouncil Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elfgzp/aicouncil"
  }
}
